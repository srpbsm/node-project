{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"node-project",
            "image":"bikeshrestha/node-project:$TAGE",
            "hostname":"node-project",
            "essential":true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":8080
                }
            ],
            "memory":400,
            "environment":[
                {
                    "name":"TAG",
                    "value":"$TAG"
                }
            ]
        }
    ]
}